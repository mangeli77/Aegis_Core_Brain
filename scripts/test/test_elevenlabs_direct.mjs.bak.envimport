import '../../_env.mjs';
// scripts/test/test_elevenlabs_direct.mjs
import fetch from 'node-fetch';

const API_KEY = 'sk_dabb5a2e38ec27e7d9de299d9b8255b59cd583c2de20bf04'; // Replace with your current key

const res = await fetch('https://api.elevenlabs.io/v1/voices', {
  method: 'GET',
  headers: {
    'xi-api-key': API_KEY,
    'Content-Type': 'application/json'
  }
});

const data = await res.json();

if (res.ok) {
  console.log(`✅ ElevenLabs key is valid.`);
  console.log(`🎤 Voices available:`, data.voices.map(v => v.name).join(', '));
} else {
  console.error(`❌ ElevenLabs rejected the key:`, data);
}